    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Perturbench Testing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/perturbench/modelcore/train.py",
            "args": [
                "data=mix_pert",
                "data.task=single_dataset_test",
                "data.data_path=./data/all_cell_line_filterdrug.h5ad",
                "model=latent_additive",
                "logger=wandb",
                "logger.wandb.project=perturbench",
                "logger.wandb.name=latent_additive",
                "trainer.log_every_n_steps=5",
                "trainer.max_epochs=10",
                "trainer.min_epochs=1",
                "trainer.devices=[0]"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src:${env:PYTHONPATH}"
            },
            "justMyCode": false,
            "stopOnEntry": false
        }
    ]
}